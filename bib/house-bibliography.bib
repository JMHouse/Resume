@InProceedings{Salsbury:Alcala:2018:decoupling,
  author        = {Salsbury, Timothy I. and House, John M. and Alcala, Carlos F.},
  title         = {Decoupling Method for PI Controllers via Setpoint Modification Applied to HVAC Systems},
  year          = {2018},
  number        = {51906},
  pages         = {V002T19A001},
  __markedentry = {[calcalca:6]},
  abstract      = {The method described in this paper addresses the problem of interaction in a control system that consists of multiple legacy PI(D) controllers. In these systems, it is often not possible to intercept the signals between the controllers and plant and replacement with multivariable alternatives such as model predictive control (MPC) may not be viable from a cost perspective. To address these issues, we propose a decoupling method that involves first changing the tuning of the controllers and then dynamically adjusting the setpoints. The method enables decoupling of a multiloop control system via setpoint adjustment which can be carried out without having to re-engineer any of the legacy controllers. The paper focuses on the application area of energy systems in buildings where costs are constrained and PID multiloop configurations are prevalent. Simulation test results are presented from two systems to demonstrate the improvement in control achieved with the method.},
  comment       = {10.1115/DSCC2018-8916},
  doi           = {https://doi.org/10.1115/dscc2018-8916},
  booktitle   = {Proceedings of the Dynamic Systems and Control Conference},
  eprinttype    = {pubmed},
  url           = {http://dx.doi.org/10.1115/DSCC2018-8916},
}


